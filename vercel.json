{
  "builds": [{
      "src": "Soninke/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
  },
  {
      "src": "setup.sh",
      "use": "@vercel/static-build",
      "config": { "distDir": "staticfiles_build" }
  }],
  "routes": [
      {
          "src": "/static/(.*)",
          "dest": "/static/$1"
      },
      {
          "src": "/(.*)",
          "dest": "Soninke/wsgi.py"
      }
  ],

  "outputDirectory": "staticfiles_build"
}